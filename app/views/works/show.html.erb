<main class="row">
    <article class="media-details">
  <h3><%= @work.title %></h3>
  <p><%= @work.creator %></p>
  <p><%= @work.publication_year %></p>
  <p><%= @work.description %></p>
  <section>
    <%= link_to("Back to media ranks", root_path, :class => "button") %>
    <%= link_to("Edit", edit_work_path(@work.id), :class => "button") %>

    <a class="button" rel="nofollow" data-method="post" href="/works/241/upvote">Upvote</a>
    <%= link_to("Upvote", vote_path(@vote.id), :class => "button") %>

    <a class="alert button" data-confirm="Are you sure?" rel="nofollow" data-method="delete" href="/works/241">Delete</a>
    <%= link_to("Delete", destroy_work_path(@work.id), :class => "button") %>
  </section>
</article>




